    "## Задание 5\n",
    "\n",
    "def count_grades():\n",
    "gradelist = {}\n",
    "\n",
    "print('Введите данные в формате: 'предмет фамилия оценка'')\n",
    "print('Для завершения введите пустую строку')\n",
    "\n",
    "while True:\n",
    "\n",
    "   input_data = input(\">>> \")n",
    "   if input_data.strip() == "":\n",
    "      break\n",
    "\n",    
    "   try:\n",
    "      subject, lastname, grade = input_data.split()\n",
    "      grade = int(grade)\n",
    "      if subject not in gradelist:\n",
    "         gradelist[subject] = {}\n",
    "      if lastname not in gradelist[subject]:\n",
    "         gradelist[subject][lastname] = []\n",
    "\n",
    "      gradelist[subject][lastname].append(grade)\n",
    "\n",
    "   except ValueError:\n",
    "      print('Неверные данные, соблюдайте указанный формат!')\n",
    "\n",
    "for subject in sorted(gradelist.keys()):\n",
    "   print(f\"\\n{subject}\")\n",
    "   for lastname in sorted(gradelist[subject].keys()):\n",
    "      grades = gradelist[subject][lastname]\n",
    "      print(f\"{lastname} {' '.join(map(str, grades))}\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "count_grades()\n",
    "\n"
   ]
