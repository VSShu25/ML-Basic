       "## Задание 2\n",
       "\n",
       "theatre = [[0,1,1,0], [1,0,0,0], [0,1,0,0]]\n",
       "tickets = int(input('Укажите количество билетов: ')) \n",
       "\n",
       "result = False\n",
       "\n",
       "for row_index, row in enumerate(theatre):\n",
       "   count = 0\n",
       "   for seat in row:\n",
       "      if seat == 0:\n",
       "         count += 1\n",
       "         if count == tickets:\n",
       "            print(row_index)\n",
       "            result = True\n",
       "            break\n",
       "         else:\n",
       "            print(False)\n",
       "   if result:\n",
       "      break\n",
       "if not result:\n",
       "   print(False)\n",
       "\n"
   ] 
